<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="mainWindow" prefHeight="170.0" prefWidth="249.0" style="-fx-background-color: #161616;" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.SignUpController">
   <children>
      <TextField fx:id="SignUpLogin" focusTraversable="false" layoutX="30.0" layoutY="21.0" promptText="Логин">
         <font>
            <Font size="15.0" />
         </font>
      </TextField>
      <PasswordField fx:id="SignUpPassword" focusTraversable="false" layoutX="30.0" layoutY="59.0" promptText="Пароль">
         <font>
            <Font size="15.0" />
         </font>
      </PasswordField>
      <Button fx:id="SignUpButton" layoutX="30.0" layoutY="111.0" mnemonicParsing="false" prefHeight="31.0" prefWidth="187.0" text="Зарегистрироваться">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
