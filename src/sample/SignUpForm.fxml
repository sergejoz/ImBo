            <Image url="@PICS/when-you-type-send-nudes-but-it-auto-corrects-to-10381134.png" />
         </image>
      </ImageView>
      <Button fx:id="ChoosePic" layoutX="30.0" layoutY="259.0" mnemonicParsing="false" text="Выбрать фото">
         <font>
            <Font size="15.0" />
