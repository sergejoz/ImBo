      <PasswordField fx:id="SignUpPassword" layoutX="30.0" layoutY="59.0" promptText="Пароль">
         <font>
            <Font size="15.0" />
         </font>
      </PasswordField>
      <ImageView fitHeight="150.0" fitWidth="200.0" layoutX="30.0" layoutY="98.0" pickOnBounds="true" preserveRatio="true">
