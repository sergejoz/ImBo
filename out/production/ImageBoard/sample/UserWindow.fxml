<AnchorPane prefHeight="292.0" prefWidth="291.0" style="-fx-background-color: #161616;" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.UserWindowController">
   <children>
      <Button fx:id="createPostButton" layoutX="22.0" layoutY="107.0" mnemonicParsing="false" prefHeight="67.0" prefWidth="217.0" text="Создать пост">
